          <stringProp name="BeanShellSampler.query">import java.text.*;
import java.io.*;
import java.util.*;


ArrayList channel_list = new ArrayList();
ArrayList users_list = new ArrayList();
String channel=null;String line=null;
BufferedReader bufRdr = new BufferedReader(new FileReader(&quot;users.txt&quot;));
while((line = bufRdr.readLine()) != null) {
        users_list.add(line);
         }
bufRdr.close();
BufferedReader bufRdr1 = new BufferedReader(new FileReader(&quot;channels.txt&quot;));
while((channel = bufRdr1.readLine()) != null) {
        channel_list.add(channel);
         }
  bufRdr1.close();
int n=0;
int userperchannel=5;
ArrayList finaluserlist = new ArrayList();
finaluserlist.addAll(users_list);
while ( (finaluserlist.size())&lt;(userperchannel*channel_list.size()))
{
finaluserlist.addAll(users_list);	
}

for (int i = 0; i &lt; channel_list.size(); i++) {
	for (int j = n; j &lt; n+userperchannel; j++) {
  log.info(i+&quot;,&quot;+channel_list.get(i)+&quot;,&quot;+finaluserlist.get(j));

  }
n=n+userperchannel;
} 
</stringProp>
